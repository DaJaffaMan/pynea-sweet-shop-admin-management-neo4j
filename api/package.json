  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "maybe-need-to-add-as-prepare-at-some-point?": "npm run generate-typings",
    "build": "nest build",
    "start": "nest start",
    "install:start:dev": "npm i && nest start --watch",
