    "@neo4j/introspector": "^1.0.3",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/graphql": "^12.0.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.12.2",
    "graphql": "^16.7.1",
    "moment": "^2.27.0",
    "neo4j-driver": "5.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "ts-morph": "^19.0.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "29.5.3",
    "@types/node": "^13.13.14",
    "@types/supertest": "^2.0.8",
    "jest": "29.1.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
